    "output" : {
        "*/*.js" : ".",

        "app.js" : {
            "main" : "./app",
            "includes" : [
                "cjs",
                "reset",
                "class",
                "message",
                "util",
                "router",
                "navigate",
                "gesture",
                "transform",
                "scroll",
                "transition",
                "page",
                "xBase",
                "xBack",
                "./app"
            ]
        }
