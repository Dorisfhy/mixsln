{
    "name" : "mix/sln",
    "root" : "#",
    "version" : "0.1.0",
    "description" : "the solution for mixjs",
    "license" : "MIT",
    "author" : "zhuxun <zhuxun.jb@taobao.com>",
    "homepage": "https://github.com/organizations/mixteam",
    "repository" : {
